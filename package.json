{
  "name": "webb-dapp",
  "version": "0.2.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=12.7.x",
    "yarn": "1.x"
  },
  "resolutions": {
    "eslint": "^6.6.0",
    "@polkadot/api": "8.7.1",
    "@polkadot/keyring": "9.4.1",
    "@polkadot/networks": "9.4.1",
    "@polkadot/types": "8.7.1",
    "@polkadot/util": "9.4.1",
    "@polkadot/util-crypto": "9.4.1",
    "@polkadot/wasm-crypto": "6.1.1",
    "react-error-overlay": "6.0.9",
    "@webb-tools/api": "0.1.4-85",
    "@webb-tools/api-derive": "0.1.4-85",
    "@webb-tools/types": "0.1.4-85",
    "@webb-tools/app-util": "0.1.4-85",
    "@webb-tools/sdk-core": "0.1.4-85",
    "@webb-tools/wasm-utils": "0.1.4-85",
    "@webb-tools/contracts": "0.2.0",
    "@webb-tools/interfaces": "0.2.0",
    "@webb-tools/anchors": "0.2.0"
  },
  "scripts": {
    "bump-version": "node scripts/bump-version.js",
    "start:dapp": "cd packages/apps && yarn webpack-cli serve --config webpack.dev.js --port 3000",
    "start:localDapp": "cd packages/apps && yarn webpack-cli serve --config webpack.dev.js --port 3000 --env LOCAL_FIXTURES=true",
    "build:dapp:production": "cd packages/apps && npx webpack --config webpack.prod.js --mode production",
    "build:dapp:development": "cd packages/apps && npx webpack --config webpack.dev.js --mode development",
    "analyze": "source-map-explorer 'packages/apps/build/static/js/*.js'",
    "test": "ts-mocha -p packages/api-providers/__test__/tsconfig.json 'packages/api-providers/__test__/**/*.spec.ts' --exit",
    "lint": "polkadot-dev-run-lint --skip-tsc",
    "ts-check": "tsc --noEmit -p ./tsconfig.json",
    "format": "prettier --write \"packages/**/*.{ts,js,jsx,tsx}\""
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@polkadot/api": "^8.7.1",
    "@polkadot/keyring": "^9.4.1",
    "@polkadot/networks": "^9.4.1",
    "@polkadot/types": "^8.7.1",
    "@polkadot/util": "^9.4.1",
    "@polkadot/util-crypto": "^9.4.1",
    "@polkadot/wasm-crypto": "^6.1.1",
    "@walletconnect/client": "^1.7.1",
    "@walletconnect/qrcode-modal": "^1.7.1",
    "@walletconnect/web3-provider": "^1.7.1",
    "D": "^1.0.0",
    "circomlibjs": "0.0.8",
    "detect-browser": "^5.2.0",
    "ethers": "^5.1.4",
    "ffjavascript": "0.2.55",
    "gh-pages": "^3.1.0",
    "history": "^5.0.0",
    "notistack": "^1.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rxjs": "7.5.2",
    "snarkjs": "0.4.22",
    "tinycolor2": "^1.4.2",
    "web3": "^1.3.5",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@hot-loader/react-dom": "^16",
    "@open-wc/webpack-import-meta-loader": "^0.4.7",
    "@polkadot/dev": "0.67.3",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@truffle/hdwallet-provider": "^1.3.0",
    "@types/enzyme": "^3.10.5",
    "@types/lodash": "^4.14.157",
    "@types/mocha": "^9.1.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/tinycolor2": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "@webb-tools/api": "^0.1.4-85",
    "@webb-tools/api-derive": "^0.1.4-85",
    "@webb-tools/types": "^0.1.4-85",
    "@webb-tools/app-util": "^0.1.4-85",
    "@webb-tools/sdk-core": "^0.1.4-85",
    "@webb-tools/wasm-utils": "^0.1.4-85",
    "babel-plugin-bundled-import-meta": "^0.3.2",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-prettier-react": "^0.0.24",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.4",
    "eslint-plugin-standard": "^4.0.1",
    "ganache-cli": "^6.12.2",
    "identity-obj-proxy": "^3.0.0",
    "mocha": "^9.1.1",
    "prettier": "^2.2.1",
    "resolve-url-loader": "5.0.0",
    "source-map-explorer": "^2.4.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.4.3"
  }
}
