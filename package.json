{
  "name": "webb-dapp",
  "version": "0.2.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=12.7.x",
    "yarn": "1.x"
  },
  "resolutions": {
	"eslint": "^6.6.0",
	"@polkadot/api": "^6.9.2",
    "@webb-tools/type-definitions": "file:../webb.js/packages/type-definitions/build",
    "@webb-tools/api": "file:../webb.js/packages/api/build",
    "@webb-tools/types": "file:../webb.js/packages/types/build",
    "@webb-tools/app-util": "file:../webb.js/packages/app-util/build",
    "@webb-tools/sdk-core": "file:../webb.js/packages/sdk-core/build",
    "@webb-tools/sdk-mixer": "file:../webb.js/packages/sdk-mixer/build",
    "@webb-tools/wasm-utils": "file:../webb.js/packages/wasm-utils/build"
  },
  "scripts": {
    "bump-version": "node scripts/bump-version.js",
    "start:dapp": "yarn run build:i18n && cd packages/apps && react-app-rewired start",
    "build:dapp": "cd packages/apps && react-app-rewired build",
    "start:dashboard": "exit 0",
    "build:dashboard": "exit 0",
    "build:i18n": "cross-env APP_NAME='dapp' node scripts/buildI18n.js",
    "release:ghpages": "gh-pages -d packages/apps/build",
    "analyze": "source-map-explorer 'packages/apps/build/static/js/*.js'",
    "test": "polkadot-dev-run-test",
    "lint": "polkadot-dev-run-lint",
    "ts-check": "tsc --noEmit -p ./tsconfig.json",
    "format": "prettier --write \"packages/**/*.{ts,js,jsx,tsx}\""
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@polkadot/networks": "^7.4.1",
    "@walletconnect/client": "^1.4.1",
    "@walletconnect/qrcode-modal": "^1.4.1",
    "@walletconnect/web3-provider": "^1.4.1",
    "circom": "^0.5.45",
	  "circomlib": "^2.0.1",
	  "circomlibjs": "^0.0.8",
	  "detect-browser": "^5.2.0",
    "ethers": "^5.1.4",
    "ffjavascript": "^0.2.38",
    "gh-pages": "^3.1.0",
    "history": "^5.0.0",
    "maci-crypto": "^0.9.1",
    "notistack": "^1.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "snarkjs": "^0.4.6",
    "tinycolor2": "^1.4.2",
    "tornado-circomlib": "git+https://github.com/webb-tools/tornado-circomlib.git",
    "tornado-snarkjs": "git+https://github.com/tornadocash/snarkjs.git#869181cfaf7526fe8972073d31655493a04326d5",
    "tornado-websnark": "git+https://github.com/webb-tools/websnark.git",
    "wasmsnark": "^0.0.10",
    "web3": "^1.3.5"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@hot-loader/react-dom": "^16",
    "@open-web3/dev-config": "^0.1.13",
    "@polkadot/dev": "^0.61.24",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@truffle/hdwallet-provider": "^1.3.0",
    "@types/enzyme": "^3.10.5",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/tinycolor2": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-plugin-bundled-import-meta": "^0.3.2",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-prettier-react": "^0.0.24",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.4",
    "eslint-plugin-standard": "^4.0.1",
    "ganache-cli": "^6.12.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.9.0",
    "jest-config": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "prettier": "^2.2.1",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.8",
    "react-hot-loader": "^4.13.0",
    "react-scripts": "^3.4.1",
    "source-map-explorer": "^2.4.2",
    "ts-jest": "^24.3.0",
    "typescript": "^4.4.3",
    "wasm-loader": "^1.3.0",
    "worker-loader": "^3.0.7"
  }
}
